{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "price": {
      "type": "float"
    },
    "product_description": {
      "model": "product-description",
      "via": "product"
    },
    "sold_by": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "user"
    },
    "category": {
      "model": "category"
    },
    "discountRate": {
      "type": "decimal"
    },
    "shipingCharge": {
      "type": "integer"
    },
    "videoURL": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "Slug": {
      "type": "uid"
    }
  }
}
